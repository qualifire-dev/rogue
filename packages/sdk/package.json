{
  "name": "@rogue/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Rogue Agent Evaluator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "build:runner": "tsc -p tsconfig.json",
    "runner:dev": "ts-node --transpile-only src/scripts/eval-runner.ts",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "rogue",
    "agent",
    "evaluator",
    "sdk",
    "typescript"
  ],
  "author": "Qualifire",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "ws": "^8.14.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qualifire/rogue-agent-evaluator"
  },
  "bugs": {
    "url": "https://github.com/qualifire/rogue-agent-evaluator/issues"
  },
  "homepage": "https://github.com/qualifire/rogue-agent-evaluator#readme"
}
